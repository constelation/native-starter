{
  "name": "NativeStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "yarn run lint",
    "prepush": "yarn run lint",
    "postcheckout": "yarn",
    "postpull": "yarn",
    "postinstall": "rndebugger-open",
    "start": "react-native start",
    "cleanstart": "watchman watch-del-all && yarn cache clean && react-native start -- --reset-cache",
    "reinstall": "rm -rf ./node_modules && watchman watch-del-all && yarn cache clean && rm -rf $TMPDIR/react-* && yarn",
    "link": "react-native link",
    "lint": "eslint src lib",
    "lint-fix": "eslint --fix src lib",
    "upgrade": "react-native-git-upgrade",
    "force-upgrade": "react-native upgrade && react-native link",
    "ios": "react-native run-ios",
    "xcode": "open ios/NativeStarter.xcodeproj",
    "android": "react-native run-android && yarn run android-logs",
    "android-emulator": "android avd",
    "android-logs": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "bundle": "react-native bundle --entry-file=index.ios.js --bundle-output=ios/main.jsbundle --dev false",
    "flow": "flow",
    "test": "jest"
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  },
  "dependencies": {
    "constelation-animate_": "^14.0.0",
    "constelation-event_": "^14.0.0",
    "constelation-image": "^13.3.1",
    "constelation-scroll-view": "^14.0.3",
    "constelation-space": "^13.0.0",
    "constelation-style_": "^14.0.0",
    "constelation-text": "^12.3.0",
    "constelation-view": "^14.0.3",
    "decko": "^1.2.0",
    "lodash": "^4.17.4",
    "mitt": "^1.1.2",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-native": "^0.42.3",
    "react-navigation": "^1.0.0-beta.7"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^1.9.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-constelation": "^1.2.0",
    "eslint-config-kylpo": "^2.0.0",
    "eslint-plugin-react": "snowypowers/eslint-plugin-react.git#max-props-per-line-fix",
    "estraverse-fb": "^1.3.1",
    "flow-bin": "0.38",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "jest-react-native": "^18.0.0",
    "mobx-remotedev": "^0.2.6",
    "prettier-eslint": "^6.1.2",
    "prettier-eslint-cli": "^3.4.2",
    "react-native-debugger-open": "^0.3.8",
    "react-test-renderer": "^15.4.2"
  },
  "jest": {
    "preset": "jest-react-native"
  }
}
